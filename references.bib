@article{ashPopulismAmbivalentAntipolarization2022,
  title = {Populism for the Ambivalent: Anti-Polarization and Support for {{Ukraine}}’s {{{\mkbibemph{Sluha Narodu}}}} Party},
  shorttitle = {Populism for the Ambivalent},
  author = {Ash, Konstantin and Shapovalov, Miroslav},
  date = {2022-11-02},
  journaltitle = {Post-Soviet Affairs},
  shortjournal = {Post-Soviet Affairs},
  volume = {38},
  number = {6},

  keywords = {Acculturation,Acculturation in social media,Analysis,Analytics,Business schools,Candidates,Case studies,Cultural factors,Digital media,Elections,eParticipation,Governance,Hashtag community,Internet,Mass media effects,Mathematical analysis,Network analysis,Polarization,Polarization in elections,Political campaigns,Political science,Presidents,Public policy,Social media,Social networks,Twitter analytics,Voter behavior,Voters,Voting},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Grover et al_2019_Polarization and acculturation in US Election 2016 outcomes – Can twitter.pdf}

@article{brunilaIdentityPoliticsEthos2018,
  title = {Identity Politics, the Ethos of Vulnerability, and Education},
  author = {Brunila, Kristiina and Rossi, Leena-Maija},
  date = {2018-02-23},
  journaltitle = {Educational Philosophy and Theory},
  shortjournal = {Educational Philosophy and Theory},
  volume = {50},

@article{artMythGlobalPopulism2022,
  title = {The {{Myth}} of {{Global Populism}}},
  author = {Art, David},
  date = {2022-09},
  journaltitle = {Perspectives on Politics},
  shortjournal = {Perspect. polit.},
  volume = {20},
  number = {3},

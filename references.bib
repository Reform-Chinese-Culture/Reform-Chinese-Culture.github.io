@article{maMotivationModelCouple2023,
  title = {A “{{Motivation}}” Model of Couple Support for Digital Technology Use among Rural Older Adults},
  author = {Ma, Jiaojiao and Cui, Jie and Zhang, Qi},
  date = {2023-02-03},
  journaltitle = {Frontiers in Psychology},
  shortjournal = {Front. Psychol.},
  volume = {14},
  pages = {1095386},
  issn = {1664-1078},
  doi = {10.3389/fpsyg.2023.1095386},
  url = {https://www.frontiersin.org/articles/10.3389/fpsyg.2023.1095386/full},
  urldate = {2024-12-14},
  abstract = {Although recent research has demonstrated spouse support for technology use among older adults, by treating them as a homogeneous group, it has overlooked differences caused by socio-demographic dimensions in their digital engagement. Following the approach of the grounded theory and interviewing 15 couples in a developing aging region (Wuzhi County, Henan, China), this study paints a fuller picture of couple support for technology adoption among older adults in terms of their psychological motivations by dividing older adults into two categories: technology supports and technology recipients. The resulting concepts of motivation (e.g., social norms, benefit driven, perceptual elements, and value satisfaction), particularly limiting motivational factors (e.g.,individual status) reveal the psychological mechanisms behind this process and are conceptualized as a “motivation” model of couple support for digital technology use among rural older adults. Our study has strong implications for active and healthy aging, as policymakers can stimulate external motivation for technology adoption among older adults by fostering a sense of family responsibility and social ethos that encourages couples to help each other. In addition, local communities as stakeholders can educate older adults about the usefulness, ease of use, and risk-averse means of digital technology, and satisfy their altruistic and egoistic psychological needs to increase the inner value satisfaction they gain from the couple support process. By doing this, motivation and engagement are thereby increased, and ultimately, technology adoption in disadvantaged socioeconomic groups may be improved.},
  langid = {english},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Ma et al_2023_A “Motivation” model of couple support for digital technology use among rural.pdf}
}

@book{manionCorruptionDesignBuilding2004,
  title = {Corruption by Design: Building Clean Government in Mainland {{China}} and {{Hong Kong}}},
  author = {Manion, Melanie},
  year = {2004;2009;},
  edition = {1},
  number = {Book, Whole},
  pages = {x},
  publisher = {Harvard University Press},
  location = {Cambridge, MA},
  doi = {10.4159/9780674040519},
  url = {https://ubc.summon.serialssolutions.com/2.0.0/link/0/eLvHCXMwfV3NT8IwFH8RuMhJwY8h4uLBG7q13VaOQiCcPJmQeGla6A6CW7IBif-9r92GQNDj2m7NL-3eR_ve7wFQ8uz1j2QCqkU18PQijCRDj0VxxllgmFG49ANlCXD-D9U5TrU7eTtD0RmhhgO0hnqo_lvVreTPNGxpxjELTRU6xkNaEvBUz6Tg4ESVNnip2pkxcZrQlPkShQ7Ouc6ryMUDm7TM2SelYppcQN0kK1zCmU5aCG5XOLcF3SLx1p3pVSwz7T65VUOaLdvQHqVZtrHywlXf7sIGclxBZzJ-H037OI0oD3ZECZdcQz1JE30LrqY8VjSaxxqtBB6GirBFpIK5LyVTklEHHveAiO3K3sjm4gCtAzcWnyj6PsV8vV36JPe-HHArxMK-WQaQivFwRCnKBxI58LAbghvV3D7IRKebXNAITSE0Hv8eERjrKOTMgdYJmA5091ur5ReUEY_57M9uQzHGWOfkN-_gvIjEMUcqXWjE-Mvre7uYPajx4bRnN1EPGq-T2cfbD9uxzks},
  abstract = {"This book contrasts the experiences of mainland China and Hong Kong to explore the pressing question of how governments can transform a culture of widespread corruption to one of clean government. Melanie Manion examines Hong Kong as the best example of the possibility of reform. Within a few years it achieved a spectacularly successful conversion to clean government. Mainland China illustrates the difficulty of reform. Despite more than two decades of anticorruption reform, corruption in China continues to spread essentially unabated."--BOOK JACKET.;Compares Hong Kong's successful anticorruption reform with mainland China's largely unsuccessful anticorruption reform efforts, shedding light on the problem of anticorruption reform in contexts where corruption is routine, effectively constituting its own informal political system. Examines Hong Kong's shift from widespread corruption in the 1960s to clean government in the 1970s, accomplished through the efforts of the Independent Commission Against Corruption. Describes an explosion of corruption in mainland China beginning in the early 1980s, leading to an equilibrium of widespread corruption by the mid-1980s. Examines problems of routine anticorruption enforcement in China, focusing on criminal corruption. Assesses the effectiveness of the episodic anticorruption campaigns used in China to address problems of routine corruption. Considers institutional designs for clean government. Manion is Professor of Public Affairs and Political Science at the University of Wisconsin. Author and subject indexes.;This book contrasts experiences of mainland China and Hong Kong to explore the pressing question of how governments can transform a culture of widespread corruption to one of clean government. Manion examines Hong Kong as the best example of the possibility of reform.;This book contrasts experiences of mainland China and Hong Kong to explore the pressing question of how governments can transform a culture of widespread corruption to one of clean government. Melanie Manion examines Hong Kong as the best example of the possibility of reform. Within a few years it achieved a spectacularly successful conversion to clean government. Mainland China illustrates the difficulty of reform. Despite more than two decades of anticorruption reform, corruption in China continues to spread essentially unabated. The book argues that where corruption is already commonplace, the context in which officials and ordinary citizens make choices to transact corruptly (or not) is crucially different from that in which corrupt practices are uncommon. A central feature of this difference is the role of beliefs about the prevalence of corruption and the reliability of government as an enforcer of rules ostensibly constraining official venality. Anticorruption reform in a setting of widespread corruption is a problem not only of reducing corrupt payoffs, but also of changing broadly shared expectations of venality. The book explores differences in institutional design choices about anticorruption agencies, appropriate incentive structures, and underlying constitutional designs that contribute to the disparate outcomes in Hong Kong and mainland China.;},

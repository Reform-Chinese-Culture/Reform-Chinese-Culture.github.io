  author = {De Visser, Maartje and Straughan, Paulin},
  date = {2021},
  publisher = {Oxford University Press},
  location = {New York},
  doi = {10.1093/oso/9780197553831.003.0015},
  url = {https://ubc.summon.serialssolutions.com/2.0.0/link/0/eLvHCXMwjV1LT8MwDLZgXNAOvMV4qQc4dmv6SssVMYHEjd0nL4m5tVIBIf49dtJO2oQQUntrozhy8n1O7C8AWTpN4q01gWGRZySWRGQLbR1pRVrg0aJ1K3JbqTrpUBqzcZxfZ7P2vRVNWOYluuCpmqlp4iVKpcx8j5GyFvn8xXOx3mZhgOKgpg5iOuGfvNfdWbcxaJH-2X4ogBvzOlz14DM_gGbo51s7lduuhLSxr8imQucCJrXdLFQIrgUeJdWNnZCf3roQPPe6TtyDkLTwSx8OYSwVEJGUJvCoH8GOa45h9wW_TuDuVa7QZs7u7qOwK286NN8RNjbysOczwE5hMX9cPDzF_U0LcVvrOq6sMYTM7DhaIo0VqtJlWK4o0TalcoXEvCQrrKkUEfOdMicOhFCk99AZpbMzGDVt484hQolHElcqm1c5psiEg0Mkkyhi4plgMYFbHsYlW-nfDTuXJti1VMXF_z67hP1Uckx8TtgVjD66T3ft9RNuvBv8AMciqhI},
  abstract = {This chapter describes Singapore’s response to the Covid-19 pandemic. The government’s strategy revolved around the two poles of technocratic and expedient governance on the one hand and social solidarity, targeted at vulnerable or weaker segments of society, on the other. A crucial factor in implementing this dual strategy is Singapore’s smallness, in spatial and demographic terms, meaning that there are natural limits to emulation by others. At the same time, Singapore’s approach was not flawless. In particular, the wildfire-like spread of the virus in migrant workers’ dormitories emerged as an embarrassing blind spot. Other serious Covid-19-related challenges remain. The most significant of these are managing the narrative to preserve high levels of government trust and a further reckoning with the stark socio-economic disparity exacerbated by the crisis. The latter in particular may be a harbinger of wider socio-political change in Singapore which will continue to unfold long after the immediate health emergency has passed.},
  isbn = {9780197553831;0197553834;},
  langid = {english},
  volumes = {Book, Section}
}

@article{doniecConsensusPolarizationHysteresis2022,
  title = {Consensus, {{Polarization}} and {{Hysteresis}} in the {{Three-State Noisy}} q-{{Voter Model}} with {{Bounded Confidence}}},
  author = {Doniec, Maciej and Lipiecki, Arkadiusz and Sznajd-Weron, Katarzyna},
  date = {2022},
  abstract = {In this work, we address the question of the role of the influence of group size on the emergence of various collective social phenomena, such as consensus, polarization and social hysteresis. To answer this question, we study the three-state noisy q-voter model with bounded confidence, in which agents can be in one of three states: two extremes (leftist and rightist) and centrist. We study the model on a complete graph within the mean-field approach and show that, depending on the size q of the influence group, saddle-node bifurcation cascades of different length appear and different collective phenomena are possible. In particular, for all values of q {$>$} 1, social hysteresis is observed. Furthermore, for small values of q ∈ (1, 4), disagreement, polarization and domination of centrists (a consensus understood as the general agreement, not unanimity) can be achieved but not the domination of extremists. The latter is possible only for larger groups of influence. Finally, by comparing our model to others, we discuss how a small change in the rules at the microscopic level can dramatically change the macroscopic behavior of the model.},
  langid = {english},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Doniec et al_2022_Consensus, Polarization and Hysteresis in the Three-State Noisy q-Voter Model.pdf}
}

@article{donovanDonaldTrumpRightwing2018,

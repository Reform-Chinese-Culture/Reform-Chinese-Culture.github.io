@article{halewoodClassAlwaysMatter2023,
  title = {‘{{Class}} Is {{Always}} a {{Matter}} of {{Morals}}’: {{Bourdieu}} and {{Dewey}} on {{Social Class}}, {{Morality}}, and {{Habit}}(Us)},
  shorttitle = {‘{{Class}} Is {{Always}} a {{Matter}} of {{Morals}}’},
  author = {Halewood, Michael},
  date = {2023-09},
  journaltitle = {Cultural Sociology},
  shortjournal = {Cultural Sociology},
  volume = {17},
  number = {3},
  pages = {373--389},
  issn = {1749-9755, 1749-9763},
  doi = {10.1177/17499755221108135},
  url = {http://journals.sagepub.com/doi/10.1177/17499755221108135},
  urldate = {2023-11-08},
  abstract = {This article argues that all judgements or statements about social class are inherently moral in that they implicitly advocate how people should (or should not) act. The argument extends Bourdieu’s linking of social class and representation by introducing Dewey’s intertwining of morality and habit. It is suggested that Kant’s apparently distinct critiques have set up three domains – knowledge, morality, aesthetics – which modern thought has treated as radically discrete. Although successful in linking the objective and the aesthetic (social class and its representation), Bourdieu was unable to incorporate the moral. Dewey’s reconceptualization of morality and habit is presented as able to overcome this limitation. The introduction of morality is intended to reflect the contingent and complex operations of social class. The article aims to destabilize contemporary conceptions of social class by clarifying the enduring moral aspect which supports its conceptualization and existence.},
  langid = {english},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Halewood_2023_‘Class is Always a Matter of Morals’.pdf}
}


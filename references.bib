@article{hoogheWhereDistrustingVoters2011,
  title = {Where {{Do Distrusting Voters Turn}} If {{There}} Is {{No Viable Exit}} or {{Voice Option}}? {{The Impact}} of {{Political Trust}} on {{Electoral Behaviour}} in the {{Belgian Regional Elections}} of {{June}} 2009},

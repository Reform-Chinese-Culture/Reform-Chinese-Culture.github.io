@article{ferdmanPerfectionistPublicSpace2018,
  title = {Perfectionist Public Space: A Political Philosophy Approach},
  author = {Ferdman, Avigail},
  date = {2018},
  journaltitle = {Space \& polity},
  volume = {22},
  number = {1},
  pages = {30--49},
  publisher = {Routledge},

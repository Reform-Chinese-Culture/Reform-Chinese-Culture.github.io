@book{bartelsDemocracyErodesTop2023,
  title = {Democracy {{Erodes}} from the {{Top}}: {{Leaders}}, {{Citizens}}, and the {{Challenge}} of {{Populism}} in {{Europe}}},
  author = {Bartels, Larry M.},
  date = {2023},
  edition = {1},

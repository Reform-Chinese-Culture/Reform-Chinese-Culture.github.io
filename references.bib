  langid = {english},
  keywords = {Analysis,Decision making,Democracy,Elections,Entropy,Government Regulation,Growth models,Humans,Jurisprudence,Laws regulations and rules,Mathematics,Models Theoretical,Municipalities,Physics,Political science,Politics,Population,Social and Behavioral Sciences,Statistical analysis,Urban Population,Voter behavior,Voters,Voting,Voting systems},

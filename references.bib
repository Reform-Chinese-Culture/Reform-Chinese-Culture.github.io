@article{millerOneNationIndivisible2012,
  title = {One {{Nation}} . . . {{Indivisible}}? {{Polarization}} in {{America Today}}},
  author = {Miller, William J.},
  editor = {Miller, William J.},
  date = {2012},
  journaltitle = {The American behavioral scientist (Beverly Hills)},
  volume = {56},
  number = {12},
  pages = {1607--1611},
  publisher = {SAGE Publications},
  location = {Los Angeles, CA},
  issn = {0002-7642},
  doi = {10.1177/0002764212463366},
  url = {https://go.exlibris.link/QvChdBRH},
  abstract = {America is an increasingly divided nation. Although politics has always been an invested process that had the potential to bring out the worst in any person, we see a different type of venom present today. We have not returned to an era of duals, but we have seen frequent breaches in decorum even in our most hallowed halls. Americans no longer seem willing or able to look past politics to assure the common good. Without getting into data, theories, or rebuttals, let us just consider America today and in recent years. In this issue of American Behavioral Scientist, I present six articles that help us gain a fuller understanding of polarization in the American polis and hopefully begin to demonstrate why American politics have become increasingly less civil in recent years. Using a variety of methodological approaches and making significantly different theoretical contributions, they help us in moving beyond the now-tired question of whether there is measurable polarization within American society. [Reprinted by permission of Sage Publications Inc., copyright holder.];America is an increasingly divided nation. Although politics has always been an invested process that had the potential to bring out the worst in any person, we see a different type of venom present today. We have not returned to an era of duals, but we have seen frequent breaches in decorum even in our most hallowed halls. Americans no longer seem willing or able to look past politics to assure the common good. Without getting into data, theories, or rebuttals, let us just consider America today and in recent years. Start with the election of president Barack Obama. After Obamas victory, he was quickly nominated for the Nobel Peace Prize simply for the change in language and ideas coming from the American president to the remainder of the international community. Rather than uniting behind Obamas winning the prize, we instead watched venomous attacks aimed at discrediting Obamas worthiness for the award. Politics aside, such an award deserves to be celebrated. Remember that Obama was only our third sitting president to claim the prize. It stretches beyond worthiness, however. During the bidding process for the 2016 Summer Olympics, some Americans celebrated in the games going to Rio de Janeiro, as they viewed the decision as a mark against Obamas legacy. Again, whether an individual agrees with the political beliefs of our chief executive or not, it is hard to not take pride in the possibility of the United States hosting the Olympics for the first time since Atlanta in 1996. Reprinted by permission of Sage Publications, Inc.;},
  langid = {english},
  keywords = {Heads of state,International community,Obama Barack,participation,Polarization,political behavior,Political ideas,Politics,Public opinion,Social Scientists,U.S.A,United States of America},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Miller_2012_One Nation.pdf}
}

@article{mittigaPoliticalLegitimacyAuthoritarianism2022,
  title = {Political {{Legitimacy}}, {{Authoritarianism}}, and {{Climate Change}}},
  author = {Mittiga, Ross},
  date = {2022-08},
  journaltitle = {American Political Science Review},
  shortjournal = {Am Polit Sci Rev},
  volume = {116},
  number = {3},
  pages = {998--1011},
  issn = {0003-0554, 1537-5943},
  doi = {10.1017/S0003055421001301},
  url = {https://www.cambridge.org/core/product/identifier/S0003055421001301/type/journal_article},
  urldate = {2023-12-10},
  abstract = {Is authoritarian power ever legitimate? The contemporary political theory literature—which largely conceptualizes legitimacy in terms of democracy or basic rights—would seem to suggest not. I argue, however, that there exists another, overlooked aspect of legitimacy concerning a government’s ability to ensure safety and security. While, under normal conditions, maintaining democracy and rights is typically compatible with guaranteeing safety, in emergency situations, conflicts between these two aspects of legitimacy can and often do arise. A salient example of this is the COVID-19 pandemic, during which severe limitations on free movement and association have become legitimate techniques of government. Climate change poses an even graver threat to public safety. Consequently, I argue, legitimacy may require a similarly authoritarian approach. While unsettling, this suggests the political importance of climate action. For if we wish to avoid legitimating authoritarian power, we must act to prevent crises from arising that can only be resolved by such means.},
  langid = {english},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Mittiga_2022_Political Legitimacy, Authoritarianism, and Climate Change.pdf;/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/Miscellaneous/Zotero-pdf/Mittiga_2022_Political Legitimacy, Authoritarianism, and Climate Change2.pdf}
}


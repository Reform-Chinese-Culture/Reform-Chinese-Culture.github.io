  date = {2020},
  journaltitle = {Australian journal of political science},
  volume = {55},
  number = {3},
  pages = {276--292},
  publisher = {Routledge},
  location = {ABINGDON},
  issn = {1036-1146;1363-030X;},
  doi = {10.1080/10361146.2020.1774507},
  url = {https://go.exlibris.link/2C5JnmPv},
  abstract = {When all citizens vote, the influence of radical parties decreases. Despite this being a central justification for compulsory voting in the past, it has been absent from contemporary debates. I examine the normative and empirical premises of the 'moderation thesis' in relation to radical right-wing populist parties today and suggest that, under certain conditions, compulsory voting can limit these parties' appeal. First, it replaces the excessive 'mobilisation' of discontented voters with a more universal mobilisation. Second, it addresses the problem of 'underrepresentation' offering a more pluralist type of representation than the populist one. And third, it reverses 'socioeconomic inequalities' that drive support for populism through the egalitarian effects that compulsory voting has on policymaking. My central thesis is this: because compulsory voting embodies inclusivist, pluralist and egalitarian values, it addresses some of the grievances that drive support for right-wing populist parties without carrying the same normative costs as populism.;When all citizens vote, the influence of radical parties decreases. Despite this being a central justification for compulsory voting in the past, it has been absent from contemporary debates. I examine the normative and empirical premises of the 'moderation thesis' in relation to radical right-wing populist parties today and suggest that, under certain conditions, compulsory voting can limit these parties' appeal. First, it replaces the excessivemobilisationof discontented voters with a more universal mobilisation. Second, it addresses the problem ofunderrepresentationoffering a more pluralist type of representation than the populist one. And third, it reversessocioeconomic inequalitiesthat drive support for populism through the egalitarian effects that compulsory voting has on policymaking. My central thesis is this: because compulsory voting embodies inclusivist, pluralist and egalitarian values, it addresses some of the grievances that drive support for right-wing populist parties without carrying the same normative costs as populism.;},
  langid = {english},
  keywords = {Attitudes,Complaints,Compulsory,Compulsory voting,Egalitarianism,Equality,Government & Law,Inequality,Justification,Moderation,Policy making,Political parties,Political Science,Political Science (excluding Public Administration Studies and Globalisation Studies),Populism,radical right,Radicalism,Representation,Right wing politics,Right-wing extremists,right-wing populism,Samhällsvetenskap,social inequalities,Social Sciences,Statsvetenskap,Statsvetenskap (exklusive studier av offentlig förvaltning och globaliseringsstudier),Voters,Voting,Voting rights},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Malkopoulou_2020_Compulsory voting and right-wing populism.pdf}
}

@article{mallesonGoodLifeEgalitarianism2022,
  title = {Good Life Egalitarianism},
  author = {Malleson, Tom},
  date = {2022-01},
  journaltitle = {Philosophy \& Social Criticism},
  shortjournal = {Philosophy \& Social Criticism},
  volume = {48},
  number = {1},
  pages = {14--39},
  issn = {0191-4537, 1461-734X},
  doi = {10.1177/0191453720931904},
  url = {http://journals.sagepub.com/doi/10.1177/0191453720931904},
  urldate = {2024-02-20},
  abstract = {This article carves out a new path between the two dominant wings of contemporary egalitarianism. The luck egalitarian emphasis on choice and personal responsibility is misplaced because individuals differ so deeply, and arbitrarily, in their choice-making capacities. Allowing inequalities to result from ‘choice’ is akin to allowing inequalities to stem from the possession of any other morally arbitrary factor – such as skin colour or gender. The move towards relational egalitarianism has been a case of two-steps forward, one-step back. While the shift away from the focus on choice is salutary, the concurrent rejection of luck is problematic, given the prevalence and importance of luck-based discrepancies in opportunities to lead a good life. A new conceptual framework is presented: good life egalitarianism. The guiding idea is that given the unavoidable arbitrariness of human capacities, the foundation for a good life should be assured for people regardless of the actual choices that they make. The essential goods necessary for leading a good life – such as the opportunities to self-determine and to enjoy non-dominating social relationships – should be guaranteed to all.},
  langid = {english},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Malleson_2022_Good life egalitarianism.pdf}
}


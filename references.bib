@article{maoRelationshipSocioeconomicConditions2023,
  title = {Relationship {{Between Socioeconomic Conditions}} and {{Financial Fraud Victimization Among Older Adults}} in {{China}}: {{Do Financial Literacy}} and {{Financial Attitudes Matter}}?},

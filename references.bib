@article{jonesPolarizationAbstentionMedian2022,
  title = {Polarization, Abstention, and the Median Voter Theorem},
  author = {Jones, Matthew I. and Sirianni, Antonio D. and Fu, Feng},
  date = {2022-02-02},
  journaltitle = {Humanities and Social Sciences Communications},
  shortjournal = {Humanit Soc Sci Commun},
  volume = {9},
  number = {1},
  pages = {1--12},
  issn = {2662-9992},
  doi = {10.1057/s41599-022-01056-0},
  url = {https://www.nature.com/articles/s41599-022-01056-0},
  urldate = {2023-11-01},
  abstract = {Abstract             The median voter theorem has long been the default model of voter behavior and candidate choice. While contemporary work on the distribution of political opinion has emphasized polarization and an increasing gap between the “left” and the “right” in democracies, the median voter theorem presents a model of anti-polarization: competing candidates move to the center of the ideological distribution to maximize vote share, regardless of the underlying ideological distribution of voters. These anti-polar results, however, largely depend on the “singled-peakedness” of voter preferences, an assumption that is rapidly losing relevance in the age of polarization. This article presents a model of voter choice that examines three potential mechanisms that can undermine this finding: a relative cost of voting that deters voters who are sufficiently indifferent to both candidates, ideologically motivated third-party alternatives that attract extreme voters, and a bimodal distribution of voter ideology. Under reasonable sets of conditions and empirically observed voter opinion distributions, these mechanisms can be sufficient to cause strategically minded candidates to fail to converge to the center, or to even become more polarized than their electorate.},
  langid = {english},
  keywords = {Candidates,Voter behavior},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Jones et al_2022_Polarization, abstention, and the median voter theorem.pdf}
}


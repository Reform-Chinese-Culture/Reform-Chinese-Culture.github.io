  keywords = {Arts & Humanities,Arts & Humanities - Other Topics,Capitalism,Doug Ford,Fashion,Ford Doug,Heteronormativity,homonationalism,Humanities Multidisciplinary,LGBTQ studies,LGBTQ2S,Neoliberalism,Ontario,participatory research,Political dissent,Populism,Youth culture},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Barry_Drak_2019_Intersectional Interventions into Queer and Trans Liberation.pdf}

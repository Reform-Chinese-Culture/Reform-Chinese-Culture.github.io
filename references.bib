@book{rhodes-purdyAgeDiscontentPopulism2023,
  title = {The {{Age}} of {{Discontent}}: {{Populism}}, {{Extremism}}, and {{Conspiracy Theories}} in {{Contemporary Democracies}}},
  shorttitle = {The {{Age}} of {{Discontent}}},
  author = {Rhodes-Purdy, Matthew and Navarre, Rachel and Utych, Stephen},
  date = {2023-03-09},
  edition = {1},
  publisher = {Cambridge University Press},
  doi = {10.1017/9781009279383},
  url = {https://www.cambridge.org/core/product/identifier/9781009279383/type/book},
  urldate = {2023-11-01},
  abstract = {The years following the 2008 financial crisis produced a surge of political discontent with populism, conspiracism, and Far Right extremism rising across the world. Despite this timing, many of these movements coalesced around cultural issues rather than economic grievances. But if culture, and not economics, is the primary driver of political discontent, why did these developments emerge after a financial collapse, a pattern that repeats throughout the history of the democratic world? Using the framework of 'Affective Political Economy', The Age of Discontent demonstrates that emotions borne of economic crises produce cultural discontent, thus enflaming conflicts over values and identities. The book uses this framework to explain the rise of populism and the radical right in the US, UK, Spain, and Brazil, and the social uprising in Chile. It argues that states must fulfill their roles as providers of social insurance and channels for citizen voices if they wish to turn back the tide of political discontent.},
  isbn = {978-1-009-27938-3 978-1-009-27939-0 978-1-009-27943-7},

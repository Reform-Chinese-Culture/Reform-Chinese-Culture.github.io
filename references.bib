@article{campionAustralianRightWing2019,
  title = {Australian Right Wing Extremist Ideology: Exploring Narratives of Nostalgia and Nemesis},
  author = {Campion, Kristy},
  date = {2019},
  journaltitle = {Journal of policing, intelligence and counter terrorism},
  volume = {14},
  number = {3},
  pages = {208--226},
  publisher = {Routledge},
  issn = {1833-5330},
  doi = {10.1080/18335330.2019.1667013},
  url = {https://ubc.summon.serialssolutions.com/2.0.0/link/0/eLvHCXMwtV3fi9QwEA6n9yKK-BNXT8mLvixd0yZNG8EHEQ998WnlHkOapFK8a4-2i-d_76Rp0-6qqIgPW5YJu6GZr5OZdOYbhGiyIdGBTTAspcIUJeUqVWlpmKYJzWD7jZWhRrP9VJ2jq6k0Zhb-V82DDHTvKmn_QvvhT0EA3wEDcAUUwPWPcLA4zBgC8fVXdzgAJrkdOr2tK2N9wYpr0RMS8mrVekpwn-nRgA95_tnXb61re2G7qvuFW-u6PuixUUq15Pv0BXQ7p8g1fNomsBeOr0AqnwAwmJ1vy9OI2KdbzbHrfhqTN6k5pZHLYfU7ziADP0OAzPOXBzvMFnijS6NK8sX-nPgK-x9Mv8-VdLO5yVzSntjEnGfE17oesGpPI45g_cJUun9t62jXXUPHEINmYOGPtx-3px_CcZ17QSoGUtdwP1M9WE5e_nTWPU9njwfX0dLq5nLX3XT7--jLbO-g26O28BuPlrvoyNb30Imv1MZn9rxUrcUv8CRo2i_30dkMJDwACTsg4QAkPAHpFQ4wwjOMcFPiACMMWMAjjB6gT6fvtm_fR2NXjkjDHfZRUhZpnlhT5IoRahINLjQxZVEKC84u0Qx8aptaoTPDVGxLCIlTrrjhRKc2yRR9iG4pV71R90OVp3mEMGWUc5EYw7lmEJAUXBSaZhA9U00FyVdoMy2lvPQsLDIeyW2ntZdu7eW49isklgsu--H4q_S9aiT9zW9XXjthKpfACa5wmsLY88Mx2SWSyBziKpoyJvJM9lf943-Y_wm6MT9ZJ-h63-7s04EU5NmIyu-wqau3},
  abstract = {Australian Right Wing Extremism (RWE) ideology can be identified by ethnocentric and structural discourses, with prominent narratives of racial identification and threat articulation. This study has examined nine RWE manifestos and collated statements of Australian authorship, both historical and contemporary, to gain a qualitative understanding of discourses, narratives and axiomatic beliefs within the heterogeneous movement, in order to identify the ideological contour of Australia's extreme right. It found that ethnocentric discourses assigned positive value to identity narratives regarding white identity and heritage and seek to proscribe the ideal society based on nostalgic and imagined identity constructs. Societal narratives situate gender roles within the ambit of a failing and degenerate society. Threat narratives assigned negative values to designated out-groups, while concurrently arguing white victimhood. Structural discourses manifested to a lesser extent, demonstrating opposition to political concepts like democracy and equality, and capitalist economic systems. Ethnocentric and structural discourses combine to elevate white identity, enforce notions of threat, and romanticise a nostalgic and imagined existence to which extremists seek a return.},
  langid = {english},
  keywords = {Australia,extremism,ideology,Right wing,terrorism},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Campion_2019_Australian right wing extremist ideology.pdf}
}


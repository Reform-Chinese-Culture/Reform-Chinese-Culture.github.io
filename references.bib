@article{klapperFinancialLiteracyFinancial2020,
  title = {Financial Literacy and Financial Resilience: {{Evidence}} from around the World},
  shorttitle = {Financial Literacy and Financial Resilience},
  author = {Klapper, Leora and Lusardi, Annamaria},
  date = {2020-09},
  journaltitle = {Financial Management},
  shortjournal = {Financial Management},
  volume = {49},
  number = {3},
  pages = {589--614},
  issn = {0046-3892, 1755-053X},
  doi = {10.1111/fima.12283},
  url = {https://onlinelibrary.wiley.com/doi/10.1111/fima.12283},
  urldate = {2024-12-14},
  abstract = {We measure financial literacy using questions assessing basic knowledge of four fundamental concepts in financial decision making: knowledge of interest rates, interest compounding, inflation, and risk diversification. Worldwide, just one in three adults are financially literate—that is, they know at least three out of the four financial concepts. Women, poor adults, and lower educated respondents are more likely to suffer from gaps in financial knowledge. This is true not only in developing countries but also in countries with welldeveloped financial markets. Relatively low financial literacy levels exacerbate consumer and financial market risks as increasingly complex financial instruments enter the market. Credit products, many of which carry high interest rates and complex terms and conditions, are becoming more readily available. Yet only around half of adults in major emerging countries who use a credit card or borrow from a financial institution are financially literate. We discuss policies to protect borrowers against risks and encourage account holders to save.},
  langid = {english},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Klapper_Lusardi_2020_Financial literacy and financial resilience.pdf}
}


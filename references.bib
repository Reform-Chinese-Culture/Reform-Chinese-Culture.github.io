  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Rhodes-Purdy et al_2023_The Age of Discontent.pdf}
}

@article{riceHowDecolonizeDemocracy2017,
  title = {How to {{Decolonize Democracy}}: {{Indigenous Governance Innovation}} in {{Bolivia}} and {{Nunavut}}, {{Canada}}},
  shorttitle = {How to {{Decolonize Democracy}}},
  author = {Rice, Roberta},
  date = {2017-03-27},
  journaltitle = {Bolivian Studies Journal},
  shortjournal = {Bolivian Studies Journal},
  pages = {220--242},
  issn = {2156-5163, 1074-2247},
  doi = {10.5195/bsj.2016.169},
  url = {http://bsj.pitt.edu/ojs/index.php/bsj/article/view/169},
  urldate = {2023-12-10},
  abstract = {This paper analyzes the successes, failures, and lessons learned from the innovative experiments in decolonization that are currently underway in Bolivia and Nunavut, Canada. Bolivia and Nunavut are the first large-scale tests of Indigenous governance in the Americas. In both cases, Indigenous peoples are a marginalized majority who have recently assumed power by way of democratic mechanisms. In Bolivia, the inclusion of direct, participatory, and communitarian elements into the democratic system, has dramatically improved representation for Indigenous peoples. In Nunavut, the Inuit have also opted to pursue self-determination through a public government system rather than through an Inuit-specific self-government arrangement. The Nunavut government seeks to incorporate Inuit values, beliefs, and worldviews into a Canadian system of government. In both cases, the conditions for success are far from ideal. Significant social, economic, and institutional problems continue to plague the new governments of Bolivia and Nunavut. Based on original research in Bolivia and Nunavut, the paper finds that important democratic gains have been made. I argue that the emergence of new mechanisms for Indigenous and popular participation has the potential to strengthen democracy by enhancing or stretching liberal democratic conceptions and expectations.},
  langid = {english},
  file = {/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/5_Miscellaneous/Zotero-pdf/Rice_2017_How to Decolonize Democracy.pdf;/Users/nasa_william_lu/Library/Mobile Documents/com~apple~CloudDocs/Miscellaneous/Zotero-pdf/Rice_2017_How to Decolonize Democracy2.pdf}
}

